<span class='kw'>def</span> <span class='id identifier rubyid_stop_service'>stop_service</span><span class='lparen'>(</span><span class='id identifier rubyid_inst'>inst</span><span class='rparen'>)</span>
  <span class='comment'># Stop the service and be done wif it, but only if the number of
</span>  <span class='comment'># references has dropped to zero
</span>  <span class='kw'>if</span> <span class='lparen'>(</span><span class='id identifier rubyid_inst'>inst</span><span class='rparen'>)</span>
    <span class='comment'># Since the instance may have multiple aliases, scan through
</span>    <span class='comment'># all the pairs for matching stuff.
</span>    <span class='kw'>self</span><span class='period'>.</span><span class='id identifier rubyid_each_pair'>each_pair</span> <span class='lbrace'>{</span> <span class='op'>|</span><span class='id identifier rubyid_cals'>cals</span><span class='comma'>,</span> <span class='id identifier rubyid_cinst'>cinst</span><span class='op'>|</span>
      <span class='kw'>self</span><span class='period'>.</span><span class='id identifier rubyid_delete'>delete</span><span class='lparen'>(</span><span class='id identifier rubyid_cals'>cals</span><span class='rparen'>)</span> <span class='kw'>if</span> <span class='lparen'>(</span><span class='id identifier rubyid_inst'>inst</span> <span class='op'>==</span> <span class='id identifier rubyid_cinst'>cinst</span><span class='rparen'>)</span>
    <span class='rbrace'>}</span>

    <span class='comment'># Lose the list-held reference to the instance
</span>    <span class='id identifier rubyid_inst'>inst</span><span class='period'>.</span><span class='id identifier rubyid_deref'>deref</span>

    <span class='kw'>return</span> <span class='kw'>true</span>
  <span class='kw'>end</span>

  <span class='comment'># Return false if the service isn&#39;t there
</span>  <span class='kw'>return</span> <span class='kw'>false</span>
<span class='kw'>end</span></pre>
