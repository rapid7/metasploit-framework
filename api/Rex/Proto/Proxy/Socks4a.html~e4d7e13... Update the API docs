<span class='kw'>def</span> <span class='id identifier rubyid_stop'>stop</span>
  <span class='kw'>if</span><span class='lparen'>(</span> <span class='ivar'>@running</span> <span class='rparen'>)</span>
    <span class='comment'># signal we are no longer running
</span>    <span class='ivar'>@running</span> <span class='op'>=</span> <span class='kw'>false</span>
    <span class='comment'># stop any clients we have (create a new client array as client.stop will delete from @clients)
</span>    <span class='id identifier rubyid_clients'>clients</span> <span class='op'>=</span> <span class='lbracket'>[</span><span class='rbracket'>]</span>
    <span class='id identifier rubyid_clients'>clients</span><span class='period'>.</span><span class='id identifier rubyid_concat'>concat</span><span class='lparen'>(</span> <span class='ivar'>@clients</span> <span class='rparen'>)</span>
    <span class='id identifier rubyid_clients'>clients</span><span class='period'>.</span><span class='id identifier rubyid_each'>each</span> <span class='kw'>do</span> <span class='op'>|</span> <span class='id identifier rubyid_client'>client</span> <span class='op'>|</span>
      <span class='id identifier rubyid_client'>client</span><span class='period'>.</span><span class='id identifier rubyid_stop'>stop</span>
    <span class='kw'>end</span>
    <span class='comment'># close the server socket
</span>    <span class='ivar'>@server</span><span class='period'>.</span><span class='id identifier rubyid_close'>close</span> <span class='kw'>if</span> <span class='ivar'>@server</span>
    <span class='comment'># if the server thread did not terminate gracefully, kill it.
</span>    <span class='ivar'>@server_thread</span><span class='period'>.</span><span class='id identifier rubyid_kill'>kill</span> <span class='kw'>if</span><span class='lparen'>(</span> <span class='ivar'>@server_thread</span> <span class='kw'>and</span> <span class='ivar'>@server_thread</span><span class='period'>.</span><span class='id identifier rubyid_alive?'>alive?</span> <span class='rparen'>)</span>
  <span class='kw'>end</span>
  <span class='kw'>return</span> <span class='op'>!</span><span class='ivar'>@running</span>
<span class='kw'>end</span></pre>
