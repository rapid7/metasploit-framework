    <span class='id identifier rubyid_encryption_mode'>encryption_mode</span> <span class='op'>=</span> <span class='id identifier rubyid_resp'>resp</span><span class='lbracket'>[</span><span class='id identifier rubyid_idx'>idx</span><span class='comma'>,</span><span class='int'>1</span><span class='rbracket'>]</span><span class='period'>.</span><span class='id identifier rubyid_unpack'>unpack</span><span class='lparen'>(</span><span class='tstring'><span class='tstring_beg'>&quot;</span><span class='tstring_content'>C</span><span class='tstring_end'>&quot;</span></span><span class='rparen'>)</span><span class='lbracket'>[</span><span class='int'>0</span><span class='rbracket'>]</span>
  <span class='kw'>else</span>
    <span class='comment'>#force to ENCRYPT_NOT_SUP and hope for the best
</span>    <span class='id identifier rubyid_encryption_mode'>encryption_mode</span> <span class='op'>=</span> <span class='const'>ENCRYPT_NOT_SUP</span>
  <span class='kw'>end</span>

  <span class='kw'>if</span> <span class='id identifier rubyid_encryption_mode'>encryption_mode</span> <span class='op'>!=</span> <span class='const'>ENCRYPT_NOT_SUP</span> <span class='kw'>and</span> <span class='id identifier rubyid_enc_error'>enc_error</span>
    <span class='id identifier rubyid_raise'>raise</span> <span class='const'>RuntimeError</span><span class='comma'>,</span><span class='tstring'><span class='tstring_beg'>&quot;</span><span class='tstring_content'>Encryption is not supported</span><span class='tstring_end'>&quot;</span></span>
  <span class='kw'>end</span>
  <span class='id identifier rubyid_encryption_mode'>encryption_mode</span>
<span class='kw'>end</span></pre>
