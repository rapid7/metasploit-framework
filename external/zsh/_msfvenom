  'r'
  'ruby'
  'sparc'
  'sparc64'
  'tty'
  'x64'
  'x86'
  'x86_64'
  'zarch'
)

_msfvenom_arch() {
  _describe -t archs 'available archs' _msfvenom_archs_list || compadd "$@"
}

